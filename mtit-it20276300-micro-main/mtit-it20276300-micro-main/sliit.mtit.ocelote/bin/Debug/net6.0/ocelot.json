{
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5071"
  },
  "Routes": [
    {
      "UpstreamPathTemplate": "/gateway/lecturer",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "DownstreamPathTemplate": "/api/lecturer",
