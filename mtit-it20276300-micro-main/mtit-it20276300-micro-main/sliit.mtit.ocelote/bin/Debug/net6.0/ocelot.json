      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5210
        }
      ]
    },

    {
      "UpstreamPathTemplate": "/gateway/feedbackAdmin",
      "UpstreamHttpMethod": [ "Get", "Post" ],
