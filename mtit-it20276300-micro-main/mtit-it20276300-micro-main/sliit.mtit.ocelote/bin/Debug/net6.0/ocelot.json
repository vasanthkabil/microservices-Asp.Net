      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5210
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/gateway/lecturer/{Id}",
      "UpstreamHttpMethod": [ "Get", "Delete", "Put" ],
