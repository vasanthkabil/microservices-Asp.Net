        {
          "Host": "localhost",
          "Port": 5220
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/gateway/feedbackAdmin/{Id}",
      "UpstreamHttpMethod": [ "Get", "Delete", "Put" ],
      "DownstreamPathTemplate": "/api/feedbackAdmin/{Id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
